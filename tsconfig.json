    "compilerOptions": {
        "target": "es5",
        "module": "none",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true
